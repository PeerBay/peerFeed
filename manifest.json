{
	"name": "A peerFeed",
	"short_name": "peerFeed",
	"description": "The web in an app.",
	"version": "1.0.0",
	"author": "PeerBay",
	"minimum_chrome_version": "36",
	"manifest_version": 2,
	"offline_enabled": true,
	"icons": {
		"512": "img/logo.png"
	},
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"permissions": [
		"notifications",
		"storage",
		"fileBrowserHandler",
		"fileSystem",
		"cookies",
		"*://*/"
	],
	"file_handlers": {
		"any": {
			"extensions": [
				"minilock"
			],
			"title": "miniLock"
		}
	}
}
